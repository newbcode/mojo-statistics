<!DOCTYPE HTML>
% layout 'index';
% title 'Chart';

<head>
  <script src="morris/third_party/jquery-1.8.2.min.js"></script>
  <script src="morris/third_party/raphael-2.1.0.min.js"></script>
  <script src="morris/morris.js"></script>
  <script src="morris/lib/prettify.js"></script>
  <script src="morris/lib/example.js"></script>
  <link rel="stylesheet" href="morris/lib/example.css">
  <link rel="stylesheet" href="morris/lib/prettify.css">
  <link rel="stylesheet" href="morris/morris.css">
</head>
<body>
<h1>Bar charts</h1>
<div id="graph"></div>
<pre id="code" class="prettyprint linenums">
<script type="text/javascript">
// Use Morris.Bar
Morris.Bar({
  element: 'graph',
  data: [
    {x: '2011 Q1', y: 0},
    {x: '2011 Q2', y: 1},
    {x: '2011 Q3', y: 2},
    {x: '2011 Q4', y: 3},
    {x: '2012 Q1', y: 4},
    {x: '2012 Q2', y: 5},
    {x: '2012 Q3', y: 6},
    {x: '2012 Q4', y: 700},
    {x: '2013 Q1', y: 8}
  ],
  xkey: 'x',
  ykeys: ['y'],
  labels: ['Y'],
  barColors: function (row, series, type) {
    if (type === 'bar') {
      var red = Math.ceil(255 * row.y / this.ymax);
      return 'rgb(' + red + ',0,0)';
    }
    else {
      return '#000';
    }
  }
});
</script>
</pre>
</body>
